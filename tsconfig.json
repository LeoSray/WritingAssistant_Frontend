{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.js",
    ".next/types/**/*.hs",
    "**/*.hs",
    "**/*.jsx",
    "src/app/(main)/homepage/page.jsx",
    "lib/mongodb.js",
    "models/users.js",
    ".next/types/**/*.ts",
    "d3.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
